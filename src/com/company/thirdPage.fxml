<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.company.Controller"
            prefHeight="600.0" prefWidth="600.0"
            stylesheets="@style3.css">
    <children>
        <Label fx:id="title1" layoutX="150.0" layoutY="19.0" text="Knapsack Solution" textFill="#FA003F"/>

        <Label fx:id="title2" layoutX="100.0" layoutY="100.0" text="Gain maximale " textFill="#047c92"/>
        <TextField fx:id="profilMax" layoutX="250.0" layoutY="100.0" prefHeight="26.0" prefWidth="130.0" />

        <Label fx:id="title3" layoutX="100.0" layoutY="140.0" text="La liste des objets qui donne le meilleur gain" textFill="#047c92"/>
        <VBox layoutX="150.0" layoutY="180.0">
            <children>
                <ListView fx:id="listsol" prefHeight="300.0" prefWidth="276.0" />
            </children>
        </VBox>

        <Button fx:id="retour_btn" layoutX="400.0" layoutY="530.0" mnemonicParsing="false" onAction="#retour"  text="Vérifier un autre cas " textFill="WHITE"></Button>
        <Button fx:id="solution_btn" layoutX="60.0" layoutY="530.0" mnemonicParsing="false" onAction="#afficherResultat"  text="Afficher le résultat " textFill="WHITE"></Button>
    </children>
</AnchorPane>
